{
  "name": "demo-ts-esm",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "license": "MIT",
  "bin": "dist/src/bin.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/src/index.js"
      }
    },
    "./*": {
      "import": {
        "types": "./dist/src/*.d.ts",
        "default": "./dist/src/*.js"
      }
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^17.0.42",
    "typescript": "^4.7.3",
    "vitest": "^0.14.2"
  },
  "packageManager": "yarn@3.2.1",
  "volta": {
    "node": "16.15.1",
    "yarn": "1.22.19"
  }
}
